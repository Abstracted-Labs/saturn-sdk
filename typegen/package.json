{
  "name": "tinkernet-typegen",
  "version": "0.0.1",
  "description": "A sample using @polkadot/typegen to generate type definitions",
  "repository": "https://github.com/jacogr/sample-polkadotjs-typegen",
  "author": "Jaco Greeff <jacogr@gmail.com>",
  "license": "GPL-3.0-only",
  "type": "module",
  "engines": {
    "node": ">=14.0.0"
  },
  "main": "./src/index.ts",
  "scripts": {
    "build": "yarn generate:defs && yarn generate:meta",
    "generate:defs": "ts-node-esm node_modules/.bin/polkadot-types-from-defs --package tinkernet-typegen/interfaces --input ./src/interfaces --endpoint ./tinkernet.json",
    "generate:meta": "ts-node-esm node_modules/.bin/polkadot-types-from-chain --package tinkernet-typegen/interfaces --endpoint ./tinkernet.json --output ./src/interfaces --strict",
    "lint": "tsc --noEmit --pretty"
  },
"devDependencies": {
    "@polkadot/typegen": "10.1.4",
    "prettier": "2.8.1",
    "ts-node": "10.9.1",
    "typescript": "4.9.4"
}
}
